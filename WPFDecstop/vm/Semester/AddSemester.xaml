<Page x:Class="WPFDecstop.vm.Semester.AddSemester"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:WPFDecstop.vm.Semester"
      mc:Ignorable="d"
      Title="AddSemester" Height="450" Width="800">
    <StackPanel>
        <Border Style="{StaticResource BorderBaseTextBlockBox}">
            <TextBlock Text="Год" Style="{StaticResource BaseTextBlock}"/>
        </Border>
        <Border Style="{StaticResource BorderBaseTextBlockBox}">
            <TextBox Name="Year" Style="{StaticResource BaseTextBox}"/>
        </Border>
        <!--<Border Style="{StaticResource BorderBaseTextBlockBox}">
            <TextBlock Text="Фамилия" Style="{StaticResource BaseTextBlock}"/>
        </Border>
        <Border Style="{StaticResource BorderBaseTextBlockBox}">
            <TextBox Name="Surname" Style="{StaticResource BaseTextBox}" />
        </Border>-->
        <Button Content="Добавить семестр" Click="ButtonBase_Add_Semester"/>
    </StackPanel>
</Page>
