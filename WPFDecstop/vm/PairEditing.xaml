<Page x:Class="WPFDecstop.vm.PairEditing"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:WPFDecstop.vm"
      mc:Ignorable="d"
      Title="Page1" Height="110" Width="180">
    <StackPanel Margin="2">
        <ComboBox Margin="1" IsEditable="True" FontSize="20" Text="Преподователь" Name="ComboBox_Teacher" SelectionChanged="ComboBox_Semester_OnSelectionChanged"></ComboBox>
        <ComboBox Margin="1" IsEditable="True" FontSize="20" Text="Группа" Name="ComboBox_Group" SelectionChanged="ComboBox_Semester_OnSelectionChanged"></ComboBox>
        <ComboBox Margin="1" IsEditable="True" FontSize="20" Text="Предмет" Name="ComboBox_Subject" SelectionChanged="ComboBox_Semester_OnSelectionChanged"></ComboBox>
    </StackPanel>
</Page>
